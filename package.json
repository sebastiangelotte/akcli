{
  "name": "@sebastian.gelotte/akcli",
  "description": "CLI for aggressive-koala",
  "version": "0.0.3",
  "author": "Sebastian Gelotte",
  "bin": {
    "akcli": "./bin/run"
  },
  "bugs": "https://github.com/sebastiangelotte/akcli/issues",
  "dependencies": {
    "@oclif/command": "^1.5.4",
    "@oclif/config": "^1.8.8",
    "@oclif/plugin-help": "^2.1.3",
    "mkdirp": "^0.5.1",
    "chalk": "^2.4.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.19.2",
    "eslint": "^5.7.0",
    "eslint-config-oclif": "^3.1.0",
    "globby": "^8.0.1",
    "rimraf": "^2.6.2"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://github.com/sebastiangelotte/akcli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "akcli",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "sebastiangelotte/akcli",
  "scripts": {
    "postpack": "rimraf oclif.manifest.json",
    "posttest": "eslint .",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
